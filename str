class HelloWorld {
    public static void main(String[] args)
    {
        String str="Yellow is my fav colour";
        String str1="Yellow";
        String str3="Yellow";
        System.out.println(str3.equals(str1));
        
         System.out.println(str3.equalsIgnoreCase(str1));
         System.out.println(str.contains("fav"));
         System.out.println(str.toUpperCase());
         System.out.println(str1.compareTo(str3));
         String str4=str1.concat(str3);
         System.out.println(str4);
        System.out.println(str.endsWith("colour"));
        System.out.println(str.indexOf("is"));
         System.out.println(str.indexOf('a'));
        char ch[]=str.toCharArray();
        for(int i=0;i<ch.length;i++)
        {
            System.out.println(ch[i]);
        }
        System.out.print(str.substring(2,5));
        System.out.println(" ");
        System.out.print(str.substring(5));
        System.out.println(" ");
        System.out.println(str.length());
        System.out.println(str.charAt(4));
        System.out.println(str1.reverse());
